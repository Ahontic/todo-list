<% @projects.each do |project| %>
	<h2> 
		<%= project.name %>
	 </h2>
	 
	 <p>
	 	<%= link_to 'Show', project_path(project) %> |
	 	<%= link_to 'Edit', edit_project_path(project) %> |
	 	<%= link_to 'Delete', project_path(project),
	 	 method: :delete,
	 	 data: { confirm: 'Are you sure?'} %>

	 </p>

<% end %>